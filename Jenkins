pipeline {
    agent any

    stages {
        stage ('Clone') {
            steps {
                print 'Clone from GIT' 
            }
        }
        
        stage ('Build'){
            steps{
                print 'Hello'
                print 'world'
            }
        }
        
        stage ('Test'){
            steps{
                print 'Hello'
                print 'world'
            }
        }  
        
        stage ('Deploy'){
            steps{
                print 'Hello'
                print 'world'
            }
        }
    }    
}
